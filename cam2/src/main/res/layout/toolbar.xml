<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:showIn="@layout/cwac_cam2_fragment">

  <View
      android:id="@+id/cwac_cam2_toolbar_overlay"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:background="@color/cwac_cam2_overlay"
      app:layout_constraintStart_toStartOf="@id/cwac_cam2_toolbar"
      app:layout_constraintEnd_toEndOf="@id/cwac_cam2_toolbar"
      app:layout_constraintTop_toTopOf="@id/cwac_cam2_toolbar"
      app:layout_constraintBottom_toBottomOf="@id/cwac_cam2_toolbar"
      android:visibility="gone"
      />
  <android.support.v7.widget.Toolbar
      android:id="@+id/cwac_cam2_toolbar"
      android:layout_width="0dp"
      android:layout_height="?android:attr/actionBarSize"
      tools:layout_constraintTop_toTopOf="@id/cwac_cam2_preview_stack"
      tools:layout_constraintStart_toStartOf="@id/cwac_cam2_preview_stack"
      tools:layout_constraintEnd_toEndOf="@id/cwac_cam2_preview_stack"
      tools:title="TITLE TITLE TITLE" />
</merge>